<UserControl
    x:Class="PicView.Views.UserControls.Logo"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    Width="25"
    Height="25"
    Margin="7,1,0,0">
    <UserControl.Resources>
        <SolidColorBrush
            x:Key="LogoBrush"
            x:Name="LogoBrush"
            Color="{StaticResource MainColor}" />
    </UserControl.Resources>
    <Image Source="pack://application:,,,/Themes/Resources/img/icon__Q6k_icon.ico" />
</UserControl>