            BorderBrush="{StaticResource BorderBrush}"
            BorderThickness="1"
            CornerRadius="2"
            Effect="{StaticResource MenuShadowButtonBorder}"
            Focusable="False">
            <Label x:Name="SetDefaultLabel" Padding="10">
                <Label.Background>
                    <SolidColorBrush x:Name="SetToDefaultBrush" Color="{StaticResource BackgroundColorAlt}" />
